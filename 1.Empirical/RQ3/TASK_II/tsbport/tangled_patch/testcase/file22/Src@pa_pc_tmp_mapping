if (children_dir_status[j].ok()) {          if (dir_index != dirs.size() - 1) {            mutex_lock lk(next_que_mutex);            next_dir_q.emplace_back(std::make_pair(child_path, dir_index));            next_que_cond.notify_one();          } else {            mutex_lock lk(results_mutex);            results->emplace_back(child_path);            results_cond.notify_one();          }
